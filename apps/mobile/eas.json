{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradle": {
          "extraPropertiesFile": "./eas-build.properties"
        }
      },
      "ios": {
        "resourceClass": "m-medium",
        "cocoapods": "1.12.1"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.payday-app.com"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-services-key.json",
        "track": "production",
        "releaseStatus": "completed",
        "rollout": 0.5
      },
      "ios": {
        "appleId": "your-apple-id@email.com",
        "ascAppId": "1234567890",
        "appleTeamId": "YOUR_TEAM_ID"
      }
    }
  }
}